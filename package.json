{
  "name": "atat",
  "version": "1.1.0",
  "author": "Sergey Golub",
  "license": "MIT",
  "description": "Fast and simple template engine",
  "keywords": [
    "fast",
    "simple",
    "template",
    "atat",
    "templating"
  ],
  "repository": "https://github.com/sgolub/atat",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "nyc mocha ./test/**/*.ts",
    "lint": "tslint --project tsconfig.json ./src/**/*.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/simple-mock": "^0.8.1",
    "expect.js": "^0.3.1",
    "express": "^4.17.1",
    "jshint": "^2.10.2",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "simple-mock": "^0.8.0",
    "source-map-support": "^0.5.13",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}
