{
  "name": "atat",
  "version": "1.2.8",
  "author": "Sergey Golub",
  "license": "MIT",
  "description": "Fast and simple template engine",
  "keywords": [
    "atat",
    "express",
    "html",
    "javascript",
    "nodejs",
    "template",
    "templating-engine",
    "templating"
  ],
  "repository": "https://github.com/sgolub/atat",
  "homepage": "https://sgolub.github.io/atat",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:coverage"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jshint": "^2.10.2",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.4"
  }
}
